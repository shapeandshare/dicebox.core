[DATASET]
name = mnist_train
dicebox_compliant = True
image_width = 28
image_height = 28
categories = 10

[TAXONOMY]
min_neurons = 1
max_neurons = 256
min_layers = 1
max_layers = 8
layer_types: ["dropout", "dense", "flatten"]
activation: ["softmax", "elu", "softplus", "softsign", "relu", "tanh", "sigmoid", "hard_sigmoid", "linear"]
# adam is currently core dumping on macos m1..
# optimizer: ["rmsprop", "adam", "sgd", "adagrad", "adadelta", "adamax", "nadam"]
optimizer: ["rmsprop", "sgd", "adagrad", "adadelta", "adamax", "nadam"]

[LONESTAR]
neurons = 1597
layers = 2
activation = relu
optimizer = nadam

[EVOLUTION]
epochs = 10
generations = 100
population = 10

[TRAINING]
# https://keras.io/api/models/model_training_apis/
batch_size = 32
train_batch_size = 2000
test_batch_size = 200
load_best_weights_on_start = False

[DIRECTORY]
dataset_base_directory = ./projects/mnist
logs_dir = ./projects/mnist/log
weights_dir = ./projects/mnist/weights
tmp_dir = ./projects/mnist/tmp
population_dir = ./projects/mnist/population

